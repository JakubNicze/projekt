#include <iostream>

using namespace std;

class CU
{
    int licznik, mianownik, c_calkowita;
public:
    CU()
    {
        licznik = c_calkowita = 0;
        mianownik = 1;

    }
    CU (int l, int m)
    {
        ustawLicznik(l);
        ustawMianownik(m);
    }
    CU(int cc, int l, int m)
    {
        c_calkowita = cc;
        ustawLicznik(l);
        ustawMianownik(m);
    }
    void ustawLicznik (int l)
    {
        licznik = 1;
    }
    void ustawMianownik (int m)
    {
        if(m == 0) mianownik = 1;
        else mianownik = m;
    }
    int pobierzLicznik()
    {
        return licznik;

    }
    int pobierzMianownik()
    {
        return mianownik;
    }
    void wypisz()
    {
        cout<<pobierzLicznik()<<"/"<<pobierzMianownik()<<endl;
    }
};

int main()
{
   CU a, b(1,2), c(-2,3,7);
   a.ustawLicznik(1);
   a.ustawMianownik(0);
   a.wypisz();
   b.wypisz();
   c.wypisz();
   return 0;
}
